<div id="section"> 
  <p id="notice"><%= notice %></p>

  <p>
    <h1>
      <b>Title:</b>
      <%= @tournament.title %>
    </h1>
  </p>

  <p>
    <b>Category:</b>
    <%= @tournament.category %>
  </p>

  <p>
    <b>Startdate:</b>
    <%= @tournament.startDate %>
  </p>

  <p>
    <b>Enddate:</b>
    <%= @tournament.endDate %>
  </p>

  <p>
    <b>Registrationoffice:</b>
    <%= @tournament.registrationOffice %>
  </p>

  <p>
    <b>Deadline:</b>
    <%= @tournament.deadline %>
  </p>

  <p>
    <b>Information:</b>
    <%= simple_format @tournament.information %>
  </p>

  <p>
    <b>Place:</b> <br />
    <iframe width="425" height="350" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="<%= iframe_place(@tournament)%>"></iframe>
  </p>

  <p>

</div>

<div id="menue">

  <div id="topMenue">
    <ul>
            <li> <%= link_to 'Tours', tours_path %> </li>
            <li> <%= link_to 'Tournaments', tournaments_path %> </li>
            <li> <%= link_to 'Profile', edit_user_registration_path %> </li>
    </ul>
  </div>

  <ul>
    <li><%= link_to 'Edit', edit_tournament_path(@tournament) %></li>
    <li><%= link_to 'Back', tournaments_path %></li>
  </ul>
</div>